if game.PlaceId == 150470833 then
    for i, v in ipairs(workspace:GetChildren()) do
        if v.Name == "8t_csCandleShop" then
            v:Destroy()
        end
    end


    for i, v in ipairs(workspace:GetChildren()) do
        if v.Name == "8t_csCandleShop" then
            v:Destroy()
        end
    end
end
